<?xml version="1.0"?>
<configuration xsi:schemaLocation="urn:hornetq /schema/hornetq-configuration.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="urn:hornetq">
	<connectors>
		<connector name="netty-connector">
			<factory-class>org.hornetq.core.remoting.impl.netty.NettyConnectorFactory
			</factory-class>
			<param key="host" value="${hornetq.remoting.netty.host:localhost}" />
			<param key="port" value="${hornetq.remoting.netty.port:5495}" />
		</connector>
	</connectors>
	<acceptors>
		<acceptor name="netty-acceptor">
			<factory-class>org.hornetq.core.remoting.impl.netty.NettyAcceptorFactory
			</factory-class>
			<param key="host" value="${hornetq.remoting.netty.host:localhost}" />
			<param key="port" value="${hornetq.remoting.netty.port:5495}" />
		</acceptor>
	</acceptors>
	<security-enabled>false</security-enabled>
</configuration>